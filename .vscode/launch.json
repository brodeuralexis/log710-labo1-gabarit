    "version": "0.2.0",
    "configurations": [
        {
            "name": "[gdb] RunCmd",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/RunCmd",
            "args": ["ls", "-al", "/usr/src"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "[make] RunCmd",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
